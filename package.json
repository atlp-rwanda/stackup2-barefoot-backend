{
  "name": "express-barefootnomad",
  "version": "1.0.0",
  "description": "Make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "start": "cross-env NODE_ENV=development && babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=development && nodemon --exec babel-node ./src/index.js",
    "admin": "babel-node ./src/database/scripts/insertAdmin.js",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test && nyc mocha --require @babel/register ./tests/**/*.js  --timeout 30000 --exit",
    "pretest": "npm run drop-db && npm run create-db && npm run migrations && npm run admin && npm run seed ",
    "posttest": "npm run undo-seed && npm run undo",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db:connect": "babel-node ./src/database/models/ connect"
  },
  "author": "Andela Simulations Program",
  "license": "MIT",
